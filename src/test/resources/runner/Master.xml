<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="test-test-suite">
    <test name="test-web" parallel="methods" thread-count="3">
        <parameter name="browser" value="chrome"/>
        <parameter name="setHeadless" value="true"/>
        <groups>
            <dependencies>
                <group name="homePage" depends-on="homePageResponse"/>
                <group name="services" depends-on="servicesPageResponse"/>
            </dependencies>
        </groups>
        <classes>
            <class name="testcase.smoke.TestSanityResponse"/>
            <class name="testcase.web.TestHomePage"/>
            <class name="testcase.web.TestServicesPage"/>
        </classes>
    </test>
</suite>
